<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginControll</servlet-name>
        <servlet-class>controller.LoginControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>controller.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Patient_Profile</servlet-name>
        <servlet-class>controller.Patient_Profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpControll</servlet-name>
        <servlet-class>controller.SignUpControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorListControl</servlet-name>
        <servlet-class>controller.admin.DoctorListControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateDoctorControl</servlet-name>
        <servlet-class>controller.admin.UpdateDoctorControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDoctorControl</servlet-name>
        <servlet-class>controller.admin.DeleteDoctorControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorFilterControl</servlet-name>
        <servlet-class>controller.admin.DoctorFilterControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorProfileControl</servlet-name>
        <servlet-class>controller.DoctorProfileControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorProfileSettting</servlet-name>
        <servlet-class>controller.DoctorProfileSettting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginControll</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpControll</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Patient_Profile</servlet-name>
        <url-pattern>/patient_profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateController</servlet-name>
        <servlet-class>controller.admin.UpdatePatientControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateController</servlet-name>
        <url-pattern>/admin/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorListControl</servlet-name>
        <url-pattern>/admin/doctor_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateDoctorControl</servlet-name>
        <url-pattern>/admin/update_doctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDoctorControl</servlet-name>
        <url-pattern>/admin/delete_doctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorFilterControl</servlet-name>
        <url-pattern>/admin/doctor_filter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorProfileControl</servlet-name>
        <url-pattern>/doctor_profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorProfileSettting</servlet-name>
        <url-pattern>/doctor_profile_setting</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            3000
        </session-timeout>
    </session-config>
</web-app>
